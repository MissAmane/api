{
  "version": 2,
  "name": "mansion-api",
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "MANSION_CONNECTION": "@mansion-connection",
    "MANSION_SECRET": "@mansion-secret"
  }
}
